   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = 0\n",
    "b = np.pi\n",
    "n = 11\n",
    "h = (b - a) / (n - 1)\n",
    "x = np.linspace (a, b, n)\n",
    "f = np.sin (x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "i_trap = (h / 2) * (f[0] + 2 * sum (f[1:n - 1] ) + f[n - 1] )\n",
    "err_trap = 2 - i_trap"
   ]
  },
